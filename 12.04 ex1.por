programa
{
	
	funcao inicio()
	{
		inteiro num[5]

		para(inteiro i = 0; i <= 4; i++)
		{
			escreva("Insira um número: ")
			leia(num[i])
		}

		limpa()

		para(inteiro i = 0; i <= 4; i++)
		{
			escreva(num[i], " ")
		}
	}
}
