programa
{

	funcao inicio()
	{
		caracter letra
		
		escreva("Insira uma letra: \n")
		leia(letra)

		//Conferir se o caracter inserido não é um núumero
		se(letra >= '0' e letra <= '9')
		{
			escreva("O caracter inserido não é uma letra")
		}


		senao
		
		{
			se(letra == 'a' ou letra == 'e' ou letra == 'i' ou letra == 'o' ou letra == 'u')
			{
				escreva("Vogal")
			}
			senao
			{
				escreva("consoante")
			}
			
			retorne;
		}

	}
}
